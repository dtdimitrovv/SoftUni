package com.company;

import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        ArrayDeque<Integer> liquidQueue = new ArrayDeque<>();
        ArrayDeque<Integer> ingredientsStack = new ArrayDeque<>();

        // Fill in the liquids queue
        int[] liquids = Arrays.stream(scanner.nextLine().split("\\s+")).mapToInt(Integer::parseInt).toArray();
        for (int liquid : liquids) {
            liquidQueue.add(liquid);
        }

        // Fill in the ingredients stack
        int[] ingredients = Arrays.stream(scanner.nextLine().split("\\s+")).mapToInt(Integer::parseInt).toArray();
        for (int ingredient : ingredients) {
            ingredientsStack.push(ingredient);
        }

        int numberOfBreads = 0;
        int numberOfCakes = 0;
        int numberOfPastries = 0;
        int numberOfFruitPies = 0;

        while (!liquidQueue.isEmpty() && !ingredientsStack.isEmpty()) {
            if (liquidQueue.peek() + ingredientsStack.peek() == 25) {
                numberOfBreads++;
                liquidQueue.poll();
                ingredientsStack.pop();
            } else if (liquidQueue.peek() + ingredientsStack.peek() == 50) {
                numberOfCakes++;
                liquidQueue.poll();
                ingredientsStack.pop();
            } else if (liquidQueue.peek() + ingredientsStack.peek() == 75) {
                numberOfPastries++;
                liquidQueue.poll();
                ingredientsStack.pop();
            } else if (liquidQueue.peek() + ingredientsStack.peek() == 100) {
                numberOfFruitPies++;
                liquidQueue.poll();
                ingredientsStack.pop();
            } else {
                liquidQueue.poll();
                ingredientsStack.push(ingredientsStack.pop() + 3);
            }
        }

        if (numberOfBreads >= 1 && numberOfCakes >= 1 && numberOfPastries >= 1 && numberOfFruitPies >= 1) {
            System.out.println("Wohoo! You succeeded in cooking all the food!");
        } else {
            System.out.println("Ugh, what a pity! You didn't have enough materials to cook everything.");
        }

        if (liquidQueue.isEmpty()) {
            System.out.println("Liquids left: none");
        } else {
            System.out.print("Liquids left: ");
            while (!liquidQueue.isEmpty()) {
                if (liquidQueue.size() != 1) {
                    System.out.print(liquidQueue.poll() + ", ");
                } else {
                    System.out.print(liquidQueue.poll());
                }
            }
            System.out.println();
        }

        if (ingredientsStack.isEmpty()) {
            System.out.println("Ingredients left: none");
        } else {
            System.out.print("Ingredients left: ");
            while (!ingredientsStack.isEmpty()) {
                if (ingredientsStack.size() != 1) {
                    System.out.print(ingredientsStack.pop() + ", ");
                } else {
                    System.out.print(ingredientsStack.pop());
                }
            }
            System.out.println();
        }

        System.out.printf("Bread: %d%n", numberOfBreads);
        System.out.printf("Cake: %d%n", numberOfCakes);
        System.out.printf("Fruit Pie: %d%n", numberOfFruitPies);
        System.out.printf("Pastry: %d", numberOfPastries);
    }
}
