package com.company;

import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        HashMap<String, String> phoneBook = new HashMap<>();

        String command;
        while (!"search".equals(command = scanner.nextLine())) {
            String[] tokes = command.split("-");

            String name = tokes[0];
            String phoneNumber = tokes[1];

            // Add it if it is absent
            phoneBook.putIfAbsent(name, phoneNumber);

            // If it is present, update the phone number
            phoneBook.put(name, phoneNumber);
        }

        while (!"stop".equals(command = scanner.nextLine())) {
            if (phoneBook.containsKey(command)) {
                System.out.println(command + " -> " + phoneBook.get(command));
            } else {
                System.out.println("Contact " + command + " does not exist.");
            }
        }
    }
}
