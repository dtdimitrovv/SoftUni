package com.company;

import java.util.Arrays;
import java.util.Scanner;
import java.util.Stack;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int numberOfFields = Integer.parseInt(scanner.nextLine());

        int[] field = new int[numberOfFields]; /// Allocate memory for the field

        int[] ladybugsPositions = Arrays.stream(scanner.nextLine().split(" ")).mapToInt(e -> Integer.parseInt(e)).toArray(); // We read the positions

        for (int i = 0; i < field.length; i++) {
            if (i < ladybugsPositions.length) { // Checks if 'i' is a valid position in the array 'ladybugsPositions'
                
                if (ladybugsPositions[i] >= 0 && ladybugsPositions[i] < field.length) {
                    // Check if the number is valid
                    // All ladybugs at invalid indexes will not be taken into account
                    
                    field[ladybugsPositions[i]] = 1; // Make the field taken
                }
            }
        }

        String command = scanner.nextLine();

        while (!command.equals("end")) {

            String[] arrayOfCommands = command.split(" "); /// Split the current command by " " so that we can use the info it brings

            int ladybugIndex = Integer.parseInt(arrayOfCommands[0]);

            String direction = arrayOfCommands[1];

            int flyLength = Integer.parseInt(arrayOfCommands[2]);

            if (ladybugIndex < 0 || ladybugIndex >= field.length) {
                // If the index at the beginning of the array of commands(the index of the ladybug) is lesser than 0 or greater than or equal to the length of the field we have to read the next command
                // The ladybug is just out of the field. Nothing happens, we read the next line

                command = scanner.nextLine(); // Read the next command

                continue; // Return in the beginning of the while loop

            } else if (field[ladybugIndex] == 0) {

                // The ladybug index that does not have ladybug there. Nothing happens, we read the next line

                command = scanner.nextLine(); // Read the next command

                continue; // Return in the beginning of the while loop
            }

            /// In other cases the ladybug will fly out. Therefore its current position becomes free
            field[ladybugIndex] = 0;

            if (direction.equals("right")) {
                ladybugIndex += flyLength; // Increase the bug index with the fly length

                // Check if the ladybug index is out the field

                while (ladybugIndex < field.length && field[ladybugIndex] != 0) {
                    // While the ladybug flies and until the place it lands is free and is on the field
                    ladybugIndex += flyLength; // Increase the bug index by the fly length
                }

                // Check if the ladybug has exited the field during flight

                if (ladybugIndex < field.length) {
                    // If it is in the field
                    field[ladybugIndex] = 1; // The space is taken
                }

            } else {
                // This means the direction is to the left

                ladybugIndex -= flyLength; // Decrease the bug index by the fly length

                while (ladybugIndex >= 0 && field[ladybugIndex] != 0) {
                    // While the ladybug flies and until the place it lands is free and is on the field
                    ladybugIndex -= flyLength; // Decrease the bug index by the fly length
                }

                // Check if the ladybug has exited the field during flight
                if (ladybugIndex >= 0) {
                    // This means the ladybug remained in the field
                    field[ladybugIndex] = 1; // The place is taken
                }

            }
            command = scanner.nextLine(); // Read the next command
        }

        // Print the result
        for (int currentField : field) {
            System.out.print(currentField + " ");
        }
    }
}
