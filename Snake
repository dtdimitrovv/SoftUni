import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int n = Integer.parseInt(scanner.nextLine());
        char[][] matrix = readMatrix(scanner, n);

        int[] snakePosition = findSnake(matrix);
        int xPosition = snakePosition[0];
        int yPosition = snakePosition[1];

        int eatenFood = 0;

        boolean outOfTerritory = false;
        while (true) {
            String moveCommand = scanner.nextLine();

            // Clear the old snake position
            matrix[xPosition][yPosition] = '.';

            if (moveCommand.equals("up")) {
                if (xPosition - 1 >= 0) {
                    xPosition--;
                } else {
                    outOfTerritory = true;
                    break;
                }
            } else if (moveCommand.equals("down")) {
                if (xPosition + 1 < matrix.length) {
                    xPosition++;
                } else {
                    outOfTerritory = true;
                    break;
                }
            } else if (moveCommand.equals("left")) {
                if (yPosition - 1 >= 0) {
                    yPosition--;
                } else {
                    outOfTerritory = true;
                    break;
                }
            } else if (moveCommand.equals("right")) {
                if (yPosition + 1 < matrix[yPosition].length) {
                    yPosition++;
                } else {
                    outOfTerritory = true;
                    break;
                }
            }

            eatenFood = checkIfSnakeIsOnFood(matrix, xPosition, yPosition, eatenFood);
            // Check if the snake is fed enough
            if (eatenFood >= 10) {
                matrix[xPosition][yPosition] = 'S';
                break;
            }

            // Check if we are on a burrow
            if (matrix[xPosition][yPosition] == 'B') {
                matrix[xPosition][yPosition] = '.';

                // Find the other burrow and change snakes position
                int[] newPosition = findLeftBurrow(matrix);
                xPosition = newPosition[0];
                yPosition = newPosition[1];
                matrix[xPosition][yPosition] = 'S';
            }
        }

        if (outOfTerritory) {
            System.out.println("Game over!");
        } else {
            System.out.println("You won! You fed the snake.");
        }

        System.out.println("Food eaten: " + eatenFood);

        printMatrix(matrix);
    }

    static void printMatrix(char[][] matrix) {
        for (int i = 0; i < matrix.length; i++) {
            for (int j = 0; j < matrix[i].length; j++) {
                System.out.print(matrix[i][j]);
            }
            System.out.println();
        }
    }

    static int[] findLeftBurrow(char[][] matrix) {
        for (int i = 0; i < matrix.length; i++) {
            for (int j = 0; j < matrix[i].length; j++) {
                if (matrix[i][j] == 'B') {
                    int[] result = new int[2];
                    result[0] = i;
                    result[1] = j;
                    return result;
                }
            }
        }
        return null;
    }

    static int checkIfSnakeIsOnFood(char[][] matrix, int xPosition, int yPosition, int eatenFood) {
        if (matrix[xPosition][yPosition] == '*') {
            eatenFood++;
        }
        return eatenFood;
    }

    static int[] findSnake(char[][] matrix) {
        for (int i = 0; i < matrix.length; i++) {
            for (int j = 0; j < matrix[i].length; j++) {
                if (matrix[i][j] == 'S') {
                    int[] result = new int[2];
                    result[0] = i;
                    result[1] = j;
                    return result;
                }
            }
        }
        return null;
    }

    static char[][] readMatrix(Scanner scanner, int n) {
        char[][] matrix = new char[n][];
        for (int i = 0; i < n; i++) {
            matrix[i] = scanner.nextLine().toCharArray();
        }
        return matrix;
    }
}
