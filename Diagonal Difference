package com.company;

import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int n = Integer.parseInt(scanner.nextLine());

        int[][] matrix = new int[n][n];

        // Fill in the matrix
        for (int row = 0; row < matrix.length; row++) {
            matrix[row] = readArray(scanner);
        }

        System.out.println(Math.abs(sumOfElementOnTheMainDiagonal(matrix) - sumOfElementOnTheSecondaryDiagonal(matrix)));
    }

    private static int[] readArray(Scanner scanner) {
        return Arrays.stream(scanner.nextLine().split("\\s+")).mapToInt(Integer::parseInt).toArray();
    }

    private static int sumOfElementOnTheMainDiagonal(int[][] matrix) {

        int sumOfElements = 0;

        for (int row = 0; row < matrix.length; row++) {

            for (int column = 0; column < matrix[row].length; column++) {

                if (row == column) {
                    sumOfElements += matrix[row][column];
                }

            }
        }

        return sumOfElements;
    }

    private static int sumOfElementOnTheSecondaryDiagonal(int[][] matrix) {

        int sumOfElements = 0;

        for (int row = matrix.length - 1; row >= 0; row--) {

            for (int column = 0; column < matrix[row].length; column++) {

                if (row + column == matrix.length - 1) {
                    sumOfElements += matrix[row][column];
                }

            }
        }

        return sumOfElements;
    }
}
