package com.company;

import java.util.Arrays;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int[] array = Arrays.stream(scanner.nextLine().split(" ")).mapToInt(e -> Integer.parseInt(e)).toArray();

        if (array.length == 1) {
            System.out.println(array[0]);
        } else {
            int[] resultArray = new int[array.length - 1]; /// Initially the result array length is equal to the input array length - 1

            for (int i = 1; resultArray.length != 1; i++) {
                resultArray = new int[array.length - 1]; /// Assign new value to resultArray

                for (int j = 0; j < array.length; j++) {
                    if (j + 1 < array.length) {
                        resultArray[j] = array[j] + array[j + 1];
                    }
                }
                array = resultArray;
            }

            System.out.println(resultArray[0]);
        }

    }
}
