package com.company;

import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // We will use LinkedHashMap as we need to print the info in the order of first appearance
        TreeMap<String, double[]> students = new TreeMap<>();
        // We will save the name of the store in the name of the student. In the array we will save its marks

        int n = Integer.parseInt(scanner.nextLine());

        for (int i = 0; i < n; i++) {

            String name = scanner.nextLine();

            double[] marks = Arrays.stream(scanner.nextLine().split("\\s+")).mapToDouble(Double::parseDouble).toArray();

            students.putIfAbsent(name, marks);
        }

        // Print the information
        for (Map.Entry<String, double[]> entry : students.entrySet()) {
            System.out.printf("%s is graduated with %s\n", entry.getKey(), countAverage(entry.getValue()));
        }
    }

    static String countAverage(double[] marks) {
        double sumOfMarks = 0;
        for (double mark : marks) {
            sumOfMarks += mark;
        }
        return String.valueOf(sumOfMarks / marks.length);
    }
}
