package com.company;

import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        TreeMap<Character, Integer> uniqueElements = new TreeMap<>();

        String input = scanner.nextLine();

        for (int i = 0; i < input.length(); i++) {
            uniqueElements.putIfAbsent(input.charAt(i), 0);

            uniqueElements.put(input.charAt(i), uniqueElements.get(input.charAt(i)) + 1);
        }

        for (Map.Entry<Character, Integer> entry : uniqueElements.entrySet()) {
            System.out.printf("%c: %d time/s\n", entry.getKey(), entry.getValue());
        }
    }
}
